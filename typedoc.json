{
  "name": "Snapper Project",
  "entryPoints": ["./**/*.ts"],
  "out": "docs/API",
  "exclude": [
    "testcases/**/*",
    "**/testcases/**",
    "**/test/**",
    "**/temp/**",
    "**/localtests/**",
    "**/scripts/**",
    "**/tests/**",
    "**/main.ts",

  ],
  "plugin": ["typedoc-plugin-markdown"],
  "hideBreadcrumbs": true,
  "includeVersion": true,
  "hideGenerator": true,
  "excludePrivate": true,
  "readme": "none"
}